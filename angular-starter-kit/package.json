{
  "name": "angular-starter-kit",
  "version": "0.0.0",
  "scripts": {
    "ng": "node ./node_modules/@angular/cli/bin/ng",
    "start": "node ./node_modules/@angular/cli/bin/ng serve",
    "build": "node ./node_modules/@angular/cli/bin/ng build",
    "build_dev": "node ./node_modules/@angular/cli/bin/ng build --base-href / --configuration development",
    "build_prod": "node ./node_modules/@angular/cli/bin/ng build --base-href / --configuration production",
    "watch": "node ./node_modules/@angular/cli/bin/ng build --watch --configuration development",
    "test": "ng test",
    "unit-test": "vitest",
    "unit-test-ui": "vitest --ui --coverage.enabled=true",
    "unit-test-coverage": "vitest run --coverage",
    "e2e": "npx playwright test",
    "e2e-ui": "npx playwright test --ui",
    "e2e-reports": "npx playwright show-report"
  },
  "prettier": {
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.0.5",
    "@angular/cdk": "^20.0.4",
    "@angular/common": "^20.0.0",
    "@angular/compiler": "^20.0.0",
    "@angular/core": "^20.0.0",
    "@angular/forms": "^20.0.0",
    "@angular/material": "^20.0.4",
    "@angular/platform-browser": "^20.0.0",
    "@angular/router": "^20.0.0",
    "@angular/service-worker": "^20.0.0",
    "@jsverse/transloco": "^7.6.1",
    "@ngrx/store": "^19.2.1",
    "@tailwindcss/postcss": "^4.1.11",
    "animate.css": "^4.1.1",
    "apexcharts": "^4.7.0",
    "hotkeys-js": "^3.13.14",
    "lodash": "^4.17.21",
    "luxon": "^3.6.1",
    "nanoid": "^5.1.5",
    "ng-apexcharts": "^1.16.0",
    "ngx-skeleton-loader": "^11.2.1",
    "ngx-toastr": "^19.0.0",
    "postcss": "^8.5.6",
    "rxjs": "~7.8.0",
    "swiper": "^11.2.10",
    "tailwindcss": "^4.1.11",
    "tslib": "^2.3.0",
    "uuid": "^11.1.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@angular/build": "^20.0.4",
    "@angular/cli": "^20.0.4",
    "@angular/compiler-cli": "^20.0.0",
    "@playwright/test": "^1.53.1",
    "@types/jasmine": "~5.1.0",
    "@types/lodash": "^4.17.19",
    "@types/luxon": "^3.6.2",
    "@types/nanoid": "^2.1.0",
    "@types/node": "^24.0.7",
    "@types/uuid": "^10.0.0",
    "@vitest/coverage-istanbul": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "jasmine-core": "~5.7.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.13",
    "typescript": "~5.8.2",
    "vitest": "^3.2.4",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
