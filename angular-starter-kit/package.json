{
  "name": "angular-starter-kit",
  "version": "0.0.0",
  "scripts": {
    "ng": "node ./node_modules/@angular/cli/bin/ng",
    "start": "node ./node_modules/@angular/cli/bin/ng serve",
    "build": "node ./node_modules/@angular/cli/bin/ng build",
    "build_dev": "node ./node_modules/@angular/cli/bin/ng build --base-href / --configuration development",
    "build_prod": "node ./node_modules/@angular/cli/bin/ng build --base-href / --configuration production",
    "watch": "node ./node_modules/@angular/cli/bin/ng build --watch --configuration development",
    "test": "ng test",
    "unit-test": "vitest",
    "unit-test-ui": "vitest --ui --coverage.enabled=true",
    "unit-test-coverage": "vitest run --coverage",
    "e2e": "npx playwright test",
    "e2e-ui": "npx playwright test --ui",
    "e2e-reports": "npx playwright show-report"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "19.0.5",
    "@angular/cdk": "19.0.4",
    "@angular/common": "19.0.5",
    "@angular/compiler": "19.0.5",
    "@angular/core": "19.0.5",
    "@angular/forms": "19.0.5",
    "@angular/material": "19.0.4",
    "@angular/platform-browser": "19.0.5",
    "@angular/platform-browser-dynamic": "19.0.5",
    "@angular/router": "19.0.5",
    "@angular/service-worker": "19.0.5",
    "@jsverse/transloco": "7.5.1",
    "@ngrx/store": "19.0.0",
    "animate.css": "4.1.1",
    "apexcharts": "4.3.0",
    "hotkeys-js": "3.13.9",
    "lodash": "4.17.21",
    "luxon": "3.5.0",
    "ng-apexcharts": "1.15.0",
    "ngx-skeleton-loader": "9.0.0",
    "ngx-toastr": "19.0.0",
    "rxjs": "7.8.1",
    "swiper": "11.2.0",
    "tslib": "2.8.1",
    "uuid": "11.0.3",
    "zod": "3.24.1",
    "zone.js": "0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "19.0.6",
    "@angular/cli": "19.0.6",
    "@angular/compiler-cli": "19.0.5",
    "@playwright/test": "1.49.1",
    "@types/jasmine": "5.1.5",
    "@types/lodash": "4.17.14",
    "@types/luxon": "3.4.2",
    "@types/node": "22.10.5",
    "@types/uuid": "10.0.0",
    "@vitest/coverage-istanbul": "2.1.8",
    "@vitest/coverage-v8": "2.1.8",
    "@vitest/ui": "2.1.8",
    "autoprefixer": "10.4.20",
    "jasmine-core": "5.5.0",
    "karma": "6.4.4",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "postcss": "8.4.49",
    "prettier": "3.4.2",
    "tailwind-merge": "2.6.0",
    "tailwindcss": "3.4.17",
    "typescript": "5.6.3",
    "vitest": "2.1.8",
    "webpack-bundle-analyzer": "4.10.2"
  }
}
