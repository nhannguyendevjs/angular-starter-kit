{
  "name": "angular-starter-kit",
  "version": "0.0.0",
  "scripts": {
    "ng": "node --max_old_space_size=10240 ./node_modules/@angular/cli/bin/ng",
    "start": "node --max_old_space_size=10240 ./node_modules/@angular/cli/bin/ng serve",
    "build": "node --max_old_space_size=10240 ./node_modules/@angular/cli/bin/ng build",
    "build_dev": "node --max_old_space_size=10240 ./node_modules/@angular/cli/bin/ng build --base-href / --configuration development",
    "build_prod": "node --max_old_space_size=10240 ./node_modules/@angular/cli/bin/ng build --base-href / --configuration production",
    "watch": "node --max_old_space_size=10240 ./node_modules/@angular/cli/bin/ng build --watch --configuration development",
    "test": "ng test",
    "unit-test": "vitest",
    "unit-test-ui": "vitest --ui --coverage.enabled=true",
    "unit-test-coverage": "vitest run --coverage",
    "e2e": "npx playwright test",
    "e2e-ui": "npx playwright test --ui",
    "e2e-reports": "npx playwright show-report"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.0",
    "@angular/cdk": "^18.2.2",
    "@angular/common": "^18.2.0",
    "@angular/compiler": "^18.2.0",
    "@angular/core": "^18.2.0",
    "@angular/forms": "^18.2.0",
    "@angular/material": "^18.2.2",
    "@angular/platform-browser": "^18.2.0",
    "@angular/platform-browser-dynamic": "^18.2.0",
    "@angular/router": "^18.2.0",
    "@angular/service-worker": "^18.2.0",
    "@jsverse/transloco": "^7.4.3",
    "@ngrx/store": "^18.0.2",
    "animate.css": "^4.1.1",
    "apexcharts": "^3.52.0",
    "hotkeys-js": "^3.13.7",
    "lodash": "^4.17.21",
    "luxon": "^3.5.0",
    "nanoid": "^5.0.7",
    "ng-apexcharts": "^1.11.0",
    "ngx-skeleton-loader": "^9.0.0",
    "ngx-toastr": "^19.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "uuid": "^10.0.0",
    "zod": "^3.23.8",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.2",
    "@angular/cli": "^18.2.2",
    "@angular/compiler-cli": "^18.2.0",
    "@playwright/test": "^1.46.1",
    "@types/jasmine": "~5.1.0",
    "@types/lodash": "^4.17.7",
    "@types/luxon": "^3.4.2",
    "@types/nanoid": "^3.0.0",
    "@types/node": "^22.5.1",
    "@types/uuid": "^10.0.0",
    "@vitest/coverage-istanbul": "^2.0.5",
    "@vitest/coverage-v8": "^2.0.5",
    "@vitest/ui": "^2.0.5",
    "autoprefixer": "^10.4.20",
    "jasmine-core": "~5.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.4.41",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.10",
    "typescript": "~5.5.2",
    "vitest": "^2.0.5",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
