{
  "name": "angular-starter-kit",
  "version": "0.0.0",
  "scripts": {
    "ng": "node ./node_modules/@angular/cli/bin/ng",
    "start": "node ./node_modules/@angular/cli/bin/ng serve",
    "build": "node ./node_modules/@angular/cli/bin/ng build",
    "build_dev": "node ./node_modules/@angular/cli/bin/ng build --base-href / --configuration development",
    "build_prod": "node ./node_modules/@angular/cli/bin/ng build --base-href / --configuration production",
    "watch": "node ./node_modules/@angular/cli/bin/ng build --watch --configuration development",
    "test": "ng test",
    "unit-test": "vitest",
    "unit-test-ui": "vitest --ui --coverage.enabled=true",
    "unit-test-coverage": "vitest run --coverage",
    "e2e": "npx playwright test",
    "e2e-ui": "npx playwright test --ui",
    "e2e-reports": "npx playwright show-report"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.0.0",
    "@angular/cdk": "^19.0.0",
    "@angular/common": "^19.0.0",
    "@angular/compiler": "^19.0.0",
    "@angular/core": "^19.0.0",
    "@angular/forms": "^19.0.0",
    "@angular/material": "^19.0.0",
    "@angular/platform-browser": "^19.0.0",
    "@angular/platform-browser-dynamic": "^19.0.0",
    "@angular/router": "^19.0.0",
    "@angular/service-worker": "^19.0.0",
    "@jsverse/transloco": "^7.5.0",
    "@ngrx/store": "^18.1.1",
    "animate.css": "^4.1.1",
    "apexcharts": "^4.0.0",
    "hotkeys-js": "^3.13.7",
    "lodash": "^4.17.21",
    "luxon": "^3.5.0",
    "nanoid": "^5.0.8",
    "ng-apexcharts": "^1.13.0",
    "ngx-skeleton-loader": "^9.0.0",
    "ngx-toastr": "^19.0.0",
    "rxjs": "~7.8.0",
    "swiper": "^11.1.15",
    "tslib": "^2.3.0",
    "uuid": "^11.0.3",
    "zod": "^3.23.8",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.0.1",
    "@angular/cli": "^19.0.1",
    "@angular/compiler-cli": "^19.0.0",
    "@playwright/test": "^1.49.0",
    "@types/jasmine": "~5.1.0",
    "@types/lodash": "^4.17.13",
    "@types/luxon": "^3.4.2",
    "@types/nanoid": "^2.1.0",
    "@types/node": "^22.9.3",
    "@types/uuid": "^10.0.0",
    "@vitest/coverage-istanbul": "^2.1.5",
    "@vitest/coverage-v8": "^2.1.5",
    "@vitest/ui": "^2.1.5",
    "autoprefixer": "^10.4.20",
    "jasmine-core": "~5.4.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.4.49",
    "prettier": "^3.3.3",
    "tailwind-merge": "^2.5.4",
    "tailwindcss": "^3.4.15",
    "typescript": "~5.6.2",
    "vitest": "^2.1.5",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
